@model IEnumerable<AssetManagement.Models.CompetencySubCategory>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubCategory_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inserted_By)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inserted_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompetencyCategory.CategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubCategory_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inserted_By)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inserted_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompetencyCategory.CategoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubCategory_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubCategory_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SubCategory_ID })
        </td>
    </tr>
}

</table>
